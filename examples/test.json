{
    "experiment": "DnaToFloatExperiment",
    "interpret_params_mode": "column_wise", 
    "transform": [
        {
            "column_name": "hello:input:dna",
            "name": ["UniformTextMasker"],
            "params": [{"probability": [0.1, 0.2]}]
        },
        {
            "column_name": "hola:label:float",
            "name": ["GaussianNoise"],
            "params": [{"mean": [0.6, 0.7], "std": [0.2, 0.3]}]
        }
    ],
    "split": [
        {
            "name": "RandomSplitter",
            "params": [{"split": [[0.6, 0.2, 0.2], [0.7, 0.15, 0.15]]}]
        }
    ]
}
